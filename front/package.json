{
  "name": "yoga-app",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:ci": "jest --coverage --ci --coverageReporters=text --coverageReporters=text-summary --coverageReporters=lcov",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng run yoga:e2e-ci",
    "e2e:coverage": "ng run yoga:e2e-ci && npx nyc report --reporter=lcov --reporter=text-summary",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.15",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/flex-layout": "15.0.0-beta.42",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.1",
    "@angular-builders/jest": "^19.0.1",
    "@angular-devkit/build-angular": "^19.2.17",
    "@angular-eslint/builder": "^20.3.0",
    "@angular-eslint/eslint-plugin": "^20.3.0",
    "@angular-eslint/eslint-plugin-template": "^20.3.0",
    "@angular-eslint/schematics": "^20.3.0",
    "@angular-eslint/template-parser": "^20.3.0",
    "@briebug/jest-schematic": "^6.0.0",
    "@cypress/code-coverage": "^3.14.6",
    "@cypress/schematic": "^4.1.3",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "cypress": "^15.2.0",
    "eslint": "^9.36.0",
    "istanbul-lib-coverage": "^3.2.2",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "jest-marbles": "^3.1.1",
    "nyc": "^17.1.0",
    "webpack": "^5.101.3"
  }
